<%- include ./layouts/head.ejs %>
<%- include ./layouts/header.ejs %>

<body>
    <div class="container">
        <h1>Estadísticas de Google Analytics</h1>
        <table class="table">
            <thead>
                <tr>
                    <th>Fecha</th>
                    <th>Sesiones</th>
                    <th>Usuarios</th>
                    <th>Vistas de página</th>
                </tr>
            </thead>
            <tbody>
                <% data.forEach(function(row) { %>
                    <tr>
                        <td><%= row.date %></td>
                        <td><%= row.sessions %></td>
                        <td><%= row.users %></td>
                        <td><%= row.pageViews %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>

    <%- include ./layouts/footer.ejs %>
</body>
